{
    "name": "ahiru-bot",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "16.13.1"
    },
    "main": "dist/start.js",
    "scripts": {
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "tsc --project tsconfig.json",
        "build:prod": "tsc --project tsconfig.production.json",
        "register": "npm run build && node --enable-source-maps --experimental-specifier-resolution=node dist/start.js --register",
        "start": "npm run build && node --enable-source-maps --experimental-specifier-resolution=node dist/start.js",
        "start:prod": "npm run build:prod && node --enable-source-maps --experimental-specifier-resolution=node dist/start.js",
        "start:shard": "npm run build && node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "type": "module",
    "exports": [
        "./dist/start.js",
        "./dist/app.js"
    ],
    "dependencies": {
        "@discordjs/rest": "0.2.0-canary.0",
        "@google-cloud/translate": "^6.3.1",
        "@google-cloud/vision": "^2.4.2",
        "@sentry/integrations": "^6.19.6",
        "@sentry/node": "^6.19.6",
        "@sentry/tracing": "^6.19.6",
        "canvas": "^2.9.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "deepl": "^1.0.13",
        "discord-api-types": "^0.31.1",
        "discord.js": "^13.6.0",
        "discord.js-rate-limiter": "1.3.2",
        "enmap": "^5.9.0",
        "express": "4.17.3",
        "express-promise-router": "4.1.1",
        "filesize": "8.0.6",
        "form-data": "^4.0.0",
        "holodex.js": "^2.0.1",
        "image-to-base64": "^2.2.0",
        "linguini": "1.3.1",
        "luxon": "2.5.2",
        "node-fetch": "3.2.10",
        "node-html-parser": "^5.3.3",
        "node-schedule": "2.1.0",
        "node-worker-threads-pool": "^1.5.1",
        "pg": "^8.7.3",
        "pino": "7.6.3",
        "pino-pretty": "7.3.0",
        "pm2": "^5.2.0",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.3.0",
        "sanitize-filename": "^1.6.3",
        "socket.io-client": "^4.4.1",
        "tslib": "^2.3.1",
        "twitter-api-v2": "^1.14.2",
        "typescript": "^4.6.3",
        "unofficial-jisho-api": "^2.3.0"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/luxon": "2.0.9",
        "@types/node": "17.0.9",
        "@types/node-fetch": "3.0.2",
        "@types/node-schedule": "1.3.2",
        "@types/pg": "^8.6.5",
        "@types/pino": "6.3.11",
        "@types/remove-markdown": "0.3.1",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.13.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-unicorn": "^40.1.0",
        "prettier": "^2.6.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    }
}
